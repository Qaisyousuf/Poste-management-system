@model OPMS.ViewModels.SidebarViewModel

@if (Model?.Id != null)
{
    <div class="panel panel-primary">
        <div class="panel-heading">
            @Model.Name
        </div>
        <div class="panel-body">
            @Html.Raw(Model.Content)
        </div>
    </div>
}

@section HomeCustomCSS{

    <link href="~/Content/HomeCustomCSS.css" rel="stylesheet" />

}